<div class="page-header">
  <h3>Job Type Match #<%= @job_type_match.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Candidate</dt>
      <dd><%= @job_type_match.candidate_id %></dd>

      <dt>Job type</dt>
      <dd><%= @job_type_match.job_type_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/job_type_matches" class="btn btn-primary">
        Back
      </a>
      <a href="/job_type_matches/<%= @job_type_match.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_job_type_match/<%= @job_type_match.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
