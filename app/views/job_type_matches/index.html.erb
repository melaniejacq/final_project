<div class="page-header">
  <h1>
    Job type matches
    <a href="/job_type_matches/new" class="btn btn-lg btn-success">New Job type match</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Candidate</th>
        <th>Job type</th>
        <th>Actions</th>
      </tr>

      <% @job_type_matches.each do |job_type_match| %>
      <tr>
        <td><%= job_type_match.job_type.name %></td>
        <td><%= job_type_match.job_type.description %></td>
        <td>
          <!--<a href="/job_type_matches/<%= job_type_match.id %>" class="btn btn-primary">Show</a>-->
          <!--<a href="/job_type_matches/<%= job_type_match.id %>/edit" class="btn btn-warning">Edit</a>-->
          <a href="/delete_job_type_match/<%= job_type_match.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
